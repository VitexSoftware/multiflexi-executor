{
  "name": "vitexsoftware/multiflexi-executor",
  "description": "Tool used to rune MuletiFlexi Jobs",
  "type": "project",
  "require": {
    "deb/multiflexi-core": "*",
    "symfony/process": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/composer/multiflexi-executor"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "MultiFlexi\\": "MultiFlexi",
      "MultiFlexi\\Env\\": "MultiFlexi/Env",
      "MultiFlexi\\Executor\\": "MultiFlexi/Executor"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/MultiFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseFluentPDO/"
    }
  ]
}
